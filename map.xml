<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over" maximum-extent="-20037508.34,-20037508.34,20037508.34,20037508.34">

  <Parameters>
    <Parameter name="format">pbf</Parameter>
    <Parameter name="json"><![CDATA[<%= JSON.stringify(json) %>]]></Parameter>
    <Parameter name="maxzoom">14</Parameter>
    <Parameter name="minzoom">0</Parameter>
  </Parameters>

  <Layer name="<%= layer %>"
    buffer-size="<%= buffer %>"
    srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">

    <Datasource>
       <Parameter name="string"><![CDATA[<%= JSON.stringify(geojson) %>]]></Parameter>
       <Parameter name="type"><![CDATA[ogr]]></Parameter>
       <Parameter name="layer">OGRGeoJson</Parameter>
    </Datasource>
  </Layer>

</Map>
